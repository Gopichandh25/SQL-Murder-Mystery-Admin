<%- layout('boilerplate') %>

<h1>crimeSceneReports</h1>
<a href="/crimeSceneReport/new" class="btn btn-primary">Add New crimeSceneReport</a>
<% for(let crimeSceneReport of crimeSceneReportList){ %>
    <div class="card" mb-3>
        <div class="row">
            <div class="col-md-8">
                <div class="card-body">
                    <h5 class="card-title">crimeSceneReport report ID : <%= crimeSceneReport.report_id %> </h5>
                    <a class="btn btn-primary" href="/crimeSceneReport/<%=crimeSceneReport.report_id%>">View crimeSceneReport details</a>
                </div>
            </div>
        </div>
    </div>

    <% } %>