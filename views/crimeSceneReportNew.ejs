<%- layout('boilerplate') %>



<div class="row">
    <div class="col-4 offset-4">
        <form action="/crimeSceneReport" method="POST" novalidate class="validated-form">
            <div class="mb-3">
                <label class="form-label" for="date">date</label>
                <input class="form-control" type="number" id="date" name="crimeSceneReport[date]" >
                <div class="valid-feedback">
                    Looks good!
                </div>
            </div>
            <div class="mb-3">
                <label class="form-label" for="type">type</label>
                <input class="form-control" type="text" id="type" name="crimeSceneReport[type]">
                <div class="valid-feedback">
                    Looks good!
                </div>
            </div>
            <div class="mb-3">
                <label class="form-label" for="description">description</label>
                <input class="form-control" type="text" id="description" name="crimeSceneReport[description]">
                <div class="valid-feedback">
                    Looks good!
                </div>
            </div>
            <div class="mb-3">
                <label class="form-label" for="city">city</label>
                <input class="form-control" type="text" id="city" name="crimeSceneReport[city]">
                <div class="valid-feedback">
                    Looks good!
                </div>
            </div>
            <div class="mb-3">
                <label class="form-label" for="report_id">report_id</label>
                <input class="form-control" type="number" id="report_id" name="crimeSceneReport[report_id]" required>
                <div class="valid-feedback">
                    Looks good!
                </div>
            </div>
           
            <div class="mb-3">
                <button class="btn btn-success">Add crimeSceneReport</button>
            </div>
        </form>
    </div>
</div>